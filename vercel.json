{
  "version": 2,
  "builds": [
    {
      "src": "client/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/index.js"
    },
    {
      "src": "/css/(.*)",
      "dest": "client/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "client/js/$1"
    },
    {
      "src": "/public/(.*)",
      "dest": "client/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "client/index.html"
    }
  ]
}
